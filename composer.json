{
    "name": "ywh/doctrine-encryption-extension",
    "type": "library",
    "description": "Doctrine2 encryption extension",
    "keywords": [
        "behaviors",
        "doctrine2",
        "extension",
        "ywh",
        "yeswehack",
        "encryption",
        "defuse"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Honel",
            "email": "r.honel@yeswehack.com"
        },
        {
            "name": "Maxime Bouchard",
            "email": "m.bouchard@yeswehack.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "defuse/php-encryption": "^2.1",
        "gedmo/doctrine-extensions": "^2.4"
    },
    "require-dev": {
        "doctrine/orm": ">=2.5.0",
        "doctrine/common": ">=2.5.0",
        "symfony/yaml": "~2.6|~3.0",
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "doctrine/orm": "to use the extensions with the ORM"
    },
    "autoload": {
        "psr-4": { "YWH\\": "lib/YWH" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
